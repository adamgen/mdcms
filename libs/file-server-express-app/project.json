{
  "root": "libs/file-server-express-app",
  "sourceRoot": "libs/file-server-express-app/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/file-server-express-app",
        "tsConfig": "libs/file-server-express-app/tsconfig.lib.json",
        "packageJson": "libs/file-server-express-app/package.json",
        "main": "libs/file-server-express-app/src/index.ts",
        "assets": ["libs/file-server-express-app/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/file-server-express-app/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/file-server-express-app"],
      "options": {
        "jestConfig": "libs/file-server-express-app/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
