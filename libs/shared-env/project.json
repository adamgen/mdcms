{
  "root": "libs/shared-env",
  "sourceRoot": "libs/shared-env/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared-env",
        "main": "libs/shared-env/src/index.ts",
        "tsConfig": "libs/shared-env/tsconfig.lib.json",
        "assets": ["libs/shared-env/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared-env/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared-env"],
      "options": {
        "jestConfig": "libs/shared-env/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
