{
  "root": "libs/fscms",
  "sourceRoot": "libs/fscms/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/fscms",
        "tsConfig": "libs/fscms/tsconfig.lib.json",
        "packageJson": "libs/fscms/package.json",
        "main": "libs/fscms/src/index.ts",
        "assets": ["libs/fscms/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/fscms/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/fscms"],
      "options": {
        "jestConfig": "libs/fscms/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
